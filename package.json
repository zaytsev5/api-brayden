{
  "name": "rental-service",
  "version": "1.0.0",
  "description": "Template Express Socket",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:goldfish-projects/template-express-socket.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha -r ts-node/register tests/**/*.test.ts",
    "clean": "rimraf ./dist/*",
    "tsc": "tsc --build tsconfig.json",
    "tsoa:gen": "tsoa routes && tsoa swagger",
    "build": "npm run clean && npm run tsoa:gen && npm run tsc && cp -r src/swagger.json dist && cp -r *.key dist && cp -r ormconfig.js dist",
    "start": "node dist/index.js",
    "start:dev": "NODE_ENV=development nodemon --watch src --watch config -e js,json,ts  --exec ./node_modules/.bin/ts-node src | ./node_modules/.bin/bunyan",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
    "dev": "tsoa routes && tsoa swagger && ts-node-dev --respawn --pretty --transpile-only src/index.ts",
    "migrate:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "migrate:up": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migrate:down": "ts-node ./node_modules/typeorm/cli.js migration:revert"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "_moduleAliases": {
    "@root": "./",
    "@controllers": "controllers",
    "@models": "models",
    "@utils": "utils",
    "@middlewares": "middlewares",
    "@routes": "routes",
    "@exceptions": "exceptions",
    "@constants": "constants",
    "@validators": "validators",
    "@configs": "configs",
    "@types": "types",
    "@services": "services"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/coinbase-commerce-node": "^1.0.6",
    "@types/config": "0.0.36",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.6",
    "@types/helmet": "0.0.45",
    "@types/hpp": "^0.2.1",
    "@types/ioredis": "^4.14.9",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/mocha": "^7.0.2",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.11.1",
    "@types/paypal-rest-sdk": "^1.7.6",
    "@types/supertest": "^2.0.8",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "bunyan": "^1.8.12",
    "chai": "^4.2.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-node": "^8.8.2",
    "ts-node-dev": "^1.1.8"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "celery-node": "^0.4.0",
    "coinbase-commerce-node": "^1.0.4",
    "config": "^3.3.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "db-migrate-mysql": "^2.1.2",
    "debug": "^4.1.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-group-routes": "^1.1.0",
    "express-session": "^1.17.2",
    "express-swaggerize-ui": "^1.1.0",
    "express-validator": "^6.12.0",
    "firebase-admin": "^10.1.0",
    "fs": "^0.0.1-security",
    "helmet": "^3.22.0",
    "hpp": "^0.2.3",
    "ioredis": "^4.16.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.7",
    "knex-paginate": "^3.0.0",
    "md5": "^2.3.0",
    "module-alias": "^2.2.2",
    "moment": "^2.27.0",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "paypal-rest-sdk": "^1.8.1",
    "reflect-metadata": "^0.1.13",
    "session-file-store": "^1.5.0",
    "stripe": "^9.1.0",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4",
    "tsoa": "^2.5.13",
    "typescript": "^4.3.5",
    "typescript-eslint-parser": "^22.0.0",
    "uuid": "^8.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
